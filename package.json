{
    "name": "typedoc-umlclass",
    "version": "0.2.2",
    "description": "Plugin for TypeDoc that generates uml class diagrams",
    "author": {
        "name": "Kriszti√°n Balla",
        "url": "https://github.com/krisztianb"
    },
    "license": "ISC",
    "keywords": [
        "uml",
        "typedoc",
        "typedocplugin"
    ],
    "dependencies": {
        "plantuml-encoder": "^1.4.0",
        "plantuml-pipe": "^1.3.3",
        "progress": "^2.0.3",
        "queue-fifo": "^0.2.6"
    },
    "devDependencies": {
        "@types/node": "14.6.2",
        "@types/plantuml-encoder": "1.4.0",
        "@types/progress": "2.0.3",
        "husky": "4.2.5",
        "prettier": "2.1.1",
        "rimraf": "3.0.2",
        "tslint": "6.1.3",
        "typedoc": "0.19.0",
        "typescript": "4.0.2"
    },
    "peerDependencies": {
        "typedoc": "^0.19.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krisztianb/typedoc-umlclass.git"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run lint && npm run format"
        }
    },
    "main": "dist/index.js",
    "files": [
        "/dist"
    ],
    "scripts": {
        "lint": "npx tslint -p .",
        "format": "npx prettier --check ./src",
        "clean": "rimraf dist",
        "build": "npx tsc",
        "clean-build": "npm run clean && npm run build"
    }
}
