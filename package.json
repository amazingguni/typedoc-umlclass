{
    "name": "typedoc-umlclass",
    "version": "0.3.0",
    "description": "Plugin for TypeDoc that generates UML class diagrams",
    "author": {
        "name": "Kriszti√°n Balla",
        "url": "https://github.com/krisztianb"
    },
    "license": "ISC",
    "keywords": [
        "uml",
        "typedoc",
        "typedocplugin"
    ],
    "dependencies": {
        "plantuml-encoder": "^1.4.0",
        "plantuml-pipe": "^1.3.4",
        "progress": "^2.0.3",
        "queue-fifo": "^0.2.6"
    },
    "devDependencies": {
        "@types/node": "14.14.2",
        "@types/plantuml-encoder": "1.4.0",
        "@types/progress": "2.0.3",
        "@typescript-eslint/eslint-plugin": "4.5.0",
        "@typescript-eslint/parser": "4.5.0",
        "eslint": "7.12.0",
        "eslint-plugin-ordered-imports": "0.3.1",
        "eslint-plugin-unicorn": "^23.0.0",
        "husky": "4.3.0",
        "prettier": "2.1.2",
        "rimraf": "3.0.2",
        "typedoc": "0.19.2",
        "typescript": "4.0.3"
    },
    "peerDependencies": {
        "typedoc": "^0.19.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krisztianb/typedoc-umlclass.git"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run lint && npm run format"
        }
    },
    "main": "dist/index.js",
    "files": [
        "/dist"
    ],
    "scripts": {
        "lint": "npx eslint ./src",
        "format": "npx prettier --check ./src",
        "clean": "rimraf dist",
        "build": "npx tsc",
        "clean-build": "npm run clean && npm run build"
    }
}
